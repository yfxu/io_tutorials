{
	"apiVersion": "1.0.0",
	"name": "Turtlebot3 Keyboard Teleoperation",
	"packageVersion": "v1.0.0",
	"description": "control the robot using the keyboard",
	"bindable": true,
	"plans": [{
		"name": "default",
