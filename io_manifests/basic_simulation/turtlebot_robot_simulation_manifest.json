{
	"apiVersion": "1.0.0",
	"name": "Turtlebot3 Robot Simulation",
	"packageVersion": "v1.0.0",
	"description": "run Gazebo simulation on the cloud",
	"bindable": true,
	"plans": [{
		"name": "default",
		"metadata": {},
		"singleton": false,
		"components": [{
			"name": "Simulator",
			"description": "",
			"cloudInfra": {
				"replicas": 1,
				"endpoints": [{
					"name": "vnc",
					"exposeExternally": true,
					"port": 443,
					"targetPort": 6901,
					"proto": "HTTPS"
				}]
			},
			"ros": {
				"topics": [{
					"name": "/cmd_vel",
					"qos": "low",
					"compression": "",
					"scoped": false,
					"targeted": false
				}, {
					"name": "/clock",
					"qos": "low",
					"compression": "",
					"scoped": false,
					"targeted": false
				}, {
					"name": "/gazebo/link_states",
					"qos": "low",
					"compression": "",
					"scoped": false,
					"targeted": false
				}, {
					"name": "/gazebo/model_states",
					"qos": "low",
					"compression": "",
					"scoped": false,
					"targeted": false
				}],
				"services": [{
					"name": "/gazebo/reset_simulation",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/spawn_urdf_model",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/spawn_sdf_model",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/delete_model",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/set_link_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/set_physics_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/set_model_state",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/set_model_configuration",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/set_joint_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/set_link_state",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_model_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_model_state",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_world_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_joint_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_link_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_link_state",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/get_physics_properties",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/link_states",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/model_states",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/apply_body_wrench",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/apply_joint_effort",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/clear_joint_forces",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/clear_body_wrenches",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/pause_physics",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/unpause_physics",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}, {
					"name": "/gazebo/reset_world",
					"timeout": 120,
					"compression": "",
					"scoped": false
				}],
				"actions": [],
				"isROS": true,
				"ros_distro": "noetic"
			},
			"requiredRuntime": "cloud",
			"architecture": "amd64",
			"executables": [{
				"name": "turtlebot3_autotrace_launcher",
				"simulationOptions": {
					"simulation": true
				},
				"gitExecutable": {
					"repository": "https://github.com/rapyuta-robotics/io_simulation_tutorials",
					"strategyType": "Source",
					"dockerFilePath": "",
					"contextDir": "turtlebot_teleoperation"
				},
				"buildOptions": {
					"catkinOptions": [{
						"rosPkgs": "turtlebot3_gazebo turtlebot3_description",
						"blacklist": "turtlebot3 turtlebot3_bringup turtlebot3_example turtlebot3_navigation turtlebot3_slam"
					}]
				},
				"cmd": ["roslaunch turtlebot3_gazebo turtlebot3_autorace.launch"],
				"limits": {
					"cpu": 2,
					"memory": 8192
				}
			}],
			"parameters": [{
				"default": "vncpassword",
				"name": "VNC_PASSWORD",
				"description": "Password for vnc endpoint"
			}]
		}],
		"includePackages": [],
		"dependentDeployments": [],
		"inboundROSInterfaces": {
			"topics": [{
				"name": "/gazebo/set_link_state"
			}, {
				"name": "/gazebo/set_model_state"
			}],
			"services": [],
			"actions": []
		},
		"exposedParameters": []
	}]
}